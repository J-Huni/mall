<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.js.mapper.homeMapper">
	<select id="modifyID" resultType="com.inhatc.vo.SignupVO">
		<![CDATA[
			select * 
			from mall_login
			where id = #{id}
		]]>
	</select>

	<select id="UserList" resultType="com.inhatc.vo.SignupVO">
		<![CDATA[
			select * 
			from mall_login
		]]>
	</select>
	
	<select id="ProductList" resultType="com.inhatc.vo.BoardVO">
		<![CDATA[
			select * 
			from mall_product
		]]>
	</select>
</mapper>